{
  "name": "prodaibeats",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "mkdir -p data && touch data/scheduler.json && echo '{\"active\":false,\"nextRun\":null,\"sources\":[],\"logs\":[]}' > data/scheduler.json && next build",
    "start": "next start",
    "lint": "next lint",
    "formatjs": "npx prettier --write 'src/**/*.js'",
    "scheduler": "node --watch --import tsx src/lib/scheduler-job.ts",
    "scheduler:run": "tsx src/lib/scheduler-job.ts",
    "scheduler:activate": "tsx scripts/activate-scheduler.ts",
    "scheduler:check-r2": "tsx scripts/check-r2-credentials.ts",
    "scheduler:status": "tsx scripts/check-scheduler-status.ts",
    "scheduler:run-now": "tsx scripts/run-scheduler-now.ts",
    "test:youtube": "node scripts/test-youtube-downloader.mjs",
    "scheduler:update": "tsx scripts/update-scheduler.mjs"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@supabase/supabase-js": "^2.49.4",
    "@types/pdfkit": "^0.13.9",
    "dotenv": "^16.5.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.7.2",
    "lucide-react": "^0.488.0",
    "next": "^14.1.4",
    "node-fetch": "^2.7.0",
    "pdfkit": "^0.17.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "resend": "^4.2.0",
    "sass": "^1.86.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.17",
    "file-loader": "^6.2.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.3",
    "typescript": "^5.3.3"
  },
  "type": "module"
}
